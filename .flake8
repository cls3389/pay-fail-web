[flake8]
max-line-length = 150
ignore = E501,W503,E402,W291,W292,W293,E203,E266,E501,W503,F401
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .env,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .coverage,
    htmlcov,
    .tox,
    .mypy_cache,
    .DS_Store,
    uploads,
    output,
    logs
